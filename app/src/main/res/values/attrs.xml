<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NoteView">
        <attr name="android:background" format="color"/>
        <attr name="android:layout_height" format="integer"/>
        <attr name="android:layout_width" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ColorCircleView">
        <attr name="circleRadius" format="dimension"/>
        <attr name="fillColor" format="color|reference"/>
        <attr name="strokeColor" format="color|reference"/>
        <attr name="strokeWidth" format="dimension"/>
    </declare-styleable>
</resources>